{
  "name": "account",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "birthday": {
      "type": "date"
    },
    "city": {
      "type": "string"
    },
    "moved": {
      "type": "date"
    },
    "profession": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "interests": {
      "type": [
        "string"
      ]
    },
    "image": {
      "type": "string"
    },
    "onboarding": {
      "type": "boolean",
      "default": false
    },
    "unsupportedcity": {
      "type": "boolean"
  },
    "tutorial": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "events": {
      "type": "hasMany",
      "model": "event",
      "foreignKey": "participantId",
      "through": "participant"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "accessToken",
      "foreignKey": "userId"
    },
    "identities": {
      "type": "hasMany",
      "model": "userIdentity",
      "foreignKey": "userId"
    },
    "credentials": {
      "type": "hasMany",
      "model": "userCredential",
      "foreignKey": "userId"
    },
    "feedbacks": {
      "type": "hasMany",
      "model": "feedback",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
